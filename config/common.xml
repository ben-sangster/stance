<?xml version="1.0" encoding="UTF-8"?>
<dmz>
<plugin-list>
   <plugin name="dmzQtModuleMainWindowBasic" scope="main-window"/>
   <plugin name="dmzQtPluginVersion"/>
   <plugin name="dmzObjectModuleBasic"/>
<!--   <plugin name="dmzObjectPluginCleanup" scope="cleanup"/>-->
   <plugin name="dmzQtPluginObjectInspector"/>
<!--   <plugin name="dmzQtPluginMessageInspector"/> -->
   <plugin name="dmzBorderWebInterface" scope="webMap"/>
</plugin-list>
<main-window>
   <corners>
      <top right="true" left="true"/>
      <bottom right="true" left="true"/>
   </corners>
   <window title="DMZ: Border Patrol Simulation"/>
   <!-- <central-widget name="canvas"/> -->
   <menu-bar>
      <menu text="&amp;File"/>
<!--
      <menu text="&amp;Edit"/>
-->
      <menu text="&amp;Window"/>
      <menu text="&amp;Help"/>
   </menu-bar>
</main-window>
<cleanup>
   <cleanup>
      <type name="current_user"/>
   </cleanup>
</cleanup>
<webMap>
   <url name="http://dev.chds.us/?dystopia:map2"/>
   <module>
      <js>
         <windowObject name="dmz"/>
      </js>
      <main-window name="dmzQtModuleMainWindowBasic"/>
   </module>
   <webview name="WebView"/>
   <message-names>
      <add name="AddPinMessage"/>
      <add-confirm name="PinAddedMessage"/>
      <remove name="RemovePinMessage"/>
      <remove-confirm name="PinRemovedMessage"/>
      <move name="MovePinMessage"/>
      <moved name="PinMovedMessage"/>
      <set-interface name="SetInterfaceWebViewMessage"/>
      <selected name="PinSelectedMessage"/>
   </message-names>
   <pin-handles>
      <id name="pinID"/>
      <position name="pinPosition"/>
      <title name="pinTitle"/>
      <description name="pinDescription"/>
      <file name="pinFile"/>
      <object-handle name="pinObjectHandle"/>
      <group-handle name="groupPinHandle"/>
      <pin-group-count name="pinGroupCountHandle"/>
   </pin-handles>
   <icon-type path="./assets/">
      <icon name="Generic" file="GenericMarker-sm.png" webfile="GenericMarker.png"/>
      <icon name="Security" file="AddSecurity-sm.png" webfile="AddSecurity.png"/>
      <icon name="Bomb" file="Bomb-sm.png" webfile="Bomb.png"/>
      <icon name="Disaster" file="Disasters-sm.png" webfile="Disasters.png"/>
      <icon name="Event" file="Event-sm.png" webfile="Event.png"/>
      <icon name="Info" file="Information-sm.png" webfile="Information.png"/>
      <icon name="Meeting" file="MeetingPlace-sm.png" webfile="MeetingPlace.png"/>
      <icon name="Note" file="Note-sm.png" webfile="Note.png"/>
      <icon name="Question" file="Placeholder-sm.png" webfile="Placeholder.png"/>
      <icon name="Target" file="Target-sm.png" webfile="Target.png"/>
      <icon name="Biohazard" file="Biohazard-sm.png" webfile="Biohazard.png"/>
      <icon name="Quarantine" file="Quarantine-sm.png" webfile="Quarantine.png"/>
      <icon name="Riot" file="Riot-sm.png" webfile="Riot.png"/>
   </icon-type>
   <pin type="map_push_pin"/>
</webMap>
</dmz>
