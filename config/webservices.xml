<?xml version="1.0" encoding="UTF-8"?>
<dmz>
<plugin-list>
   <plugin name="dmzWebServicesModuleQt" scope="web-services"/>
   <plugin name="dmzWebServicesPluginObject" scope="web-services-object"/>
<!--   <plugin name="dmzQtPluginLoginDialog" scope="login-dialog"/>-->
   <plugin name="dmzQtPluginLoginChds" scope="chds-login"/>
</plugin-list>
<web-services>
<!--   <server host="localhost" port="5984">-->
<!--   <server host="dmz.couchone.com" port="80">-->
   <server host="games.chds.us" port="443" path="/db/">
<!--      <proxy host="localhost" port="8888"/>-->
   </server>
<!--   <login-target name="dmzQtPluginLoginDialog"/>-->
   <login-target name="dmzQtPluginLoginChds"/>
</web-services>
<web-services-object>
   <filter>
      <object-type-set>
<!--         <object-type name="map_push_pin"/>-->
      </object-type-set>
      <attribute name="Active">
         <mask name="flag"/>
      </attribute>
      <attribute name="server_time">
         <mask name="timestamp"/>
      </attribute>
      <attribute name="Object_Human_In_The_Loop">
         <mask name="flag"/>
      </attribute>
      <!-- <state name="Selected"/> -->
      <!-- <state name="Highlight"/> -->
   </filter>
   <db app="stance_testing"/>
</web-services-object>
<login-dialog>
   <login-target name="dmzWebServicesModuleQt"/>
<!--   <auto-login value="true"/>-->
</login-dialog>
<chds-login>
   <login-target name="dmzWebServicesModuleQt"/>
   <picture-grid>
      <row>
         <col value="Bell"/>
         <col value="House"/>
         <col value="Microphone"/>
         <col value="ShoppingBag"/>
      </row>
      <row>
         <col value="Yield"/>
         <col value="Film"/>
         <col value="Wallet"/>
         <col value="Trees"/>
      </row>
      <row>
         <col value="Camera"/>
         <col value="Box"/>
         <col value="Stoplight"/>
         <col value="Phone"/>
         </row>
      <row>
         <col value="Chip"/>
         <col value="WallClock"/>
         <col value="Burn"/>
         <col value="Smile"/>
      </row>
      <row>
         <col value="Suit"/>
         <col value="MusicalNote"/>
         <col value="Water"/>
         <col value="Coffee"/>
      </row>
   </picture-grid>
   <color-grid>
      <row>
         <col value="Red"/>
         <col value="Green"/>
         <col value="Blue"/>
         <col value="Yellow"/>
      </row>
      <row>
         <col value="Pink"/>
         <col value="Black"/>
         <col value="Purple"/>
         <col value="Orange"/>
      </row>
   </color-grid>
</chds-login>
