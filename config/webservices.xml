<?xml version="1.0" encoding="UTF-8"?>
<dmz>
<plugin-list>
   <plugin name="dmzWebServicesModuleQt" scope="web-services"/>
   <plugin name="dmzWebServicesPluginObject" scope="web-services-object"/>
   <plugin name="dmzQtPluginLoginDialog" scope="login-dialog"/>
</plugin-list>
<web-services>
  <server db="stance">
  <!-- <server host="dmz.couchone.com" db="test"> -->
  <!-- <server host="games.chds.us" port="443" path="/db/" db="test"> -->
    <!-- <proxy host="localhost" port="8888"/> -->
  </server>
  <login-target name="dmzQtPluginLoginDialog"/>
</web-services>
<web-services-object>
   <filter>
      <object-type-set>
         <object-type name="forum_type"/>
         <object-type name="forum_post"/>
         <object-type name="game_type"/>
         <object-type name="vote"/>
         <object-type name="intel"/>
         <object-type name="current_user"/>
         <object-type name="map_push_pin"/>
      </object-type-set>
      <!-- <attribute name="Object_Select_Attribute">
         <mask name="flag"/>
      </attribute> -->
      <!-- <state name="Selected"/> -->
      <!-- <state name="Highlight"/> -->
   </filter>
   <message
      login-success="LoginSuccessMessage"
      login-failed="LoginFailedMessage"
      logout="LogoutMessage">
   </message>
</web-services-object>
<login-dialog>
   <username name="username"/>
   <password name="password"/>
   <message
      login-required="LoginRequiredMessage"
      login="LoginMessage"
      login-success="LoginSuccessMessage"
      login-failed="LoginFailedMessage"
      logout="LogoutMessage">
   </message>
   <login-target name="dmzWebServicesModuleQt"/>
<!--   <auto-login value="true"/>-->
</login-dialog>
